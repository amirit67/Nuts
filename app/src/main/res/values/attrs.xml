<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>


    <declare-styleable name="amir">
        <attr name="nvmIcon" format="reference" />
        <attr name="nvmIconHeight" format="dimension" />
        <attr name="nvmIconWith" format="dimension" />
        <attr name="nvmTitle" format="string" />
        <attr name="nvmTitleColor" format="reference" />
        <attr name="nvmTitleGravity">
            <enum name="CENTER" value="17" />
            <enum name="LEFT" value="3" />
            <enum name="RIGHT" value="5" />
        </attr>
        <attr name="nvmTitleHeight" format="dimension" />
        <attr name="nvmTitleMarginBottom" format="dimension" />
        <attr name="nvmTitleMarginLeft" format="dimension" />
        <attr name="nvmTitleMarginRight" format="dimension" />
        <attr name="nvmTitleMarginTop" format="dimension" />
        <attr name="nvmTitleSize" format="dimension" />
        <attr name="nvmTitleWith" format="dimension" />

        <attr name="nvmType">
            <enum name="NOTIFICATION" value="263" />
            <enum name="WALLET" value="133" />
            <enum name="STATISTIC" value="253" />
            <enum name="CALL_ANSWER" value="223" />
            <enum name="CONTACTS" value="203" />
            <enum name="FAVORITE" value="123" />
            <enum name="HELP" value="243" />
            <enum name="MY_PROFILE_REPLAY_COMMENT" value="183" />
            <enum name="NOTIFICATIONS" value="193" />
            <enum name="TRANSACTION" value="300" />
            <enum name="PRICE_LIST" value="213" />
            <enum name="REQUEST_MASTER" value="163" />
            <enum name="CONTACT_US" value="153" />
            <enum name="USER_JOBS" value="113" />
            <enum name="VACATION" value="173" />
            <enum name="ABOUT_US" value="143" />
            <enum name="BASKET" value="273" />
            <enum name="EXIT" value="283" />
            <enum name="GUIDE" value="293" />
            <enum name="REGISTER_ADS" value="295" />
        </attr>
    </declare-styleable>




    <declare-styleable name="ShadowLayout">
        <attr name="sl_shadowed" format="boolean"/>
        <attr name="sl_shadow_distance" format="dimension"/>
        <attr name="sl_shadow_angle" format="integer"/>
        <attr name="sl_shadow_radius" format="dimension"/>
        <attr name="sl_shadow_color" format="color"/>
    </declare-styleable>


    <declare-styleable name="ShadowView">
        <attr name="android:foreground"/>
        <attr name="shadowMargin" format="dimension"/>
        <attr name="shadowMarginTop" format="dimension"/>
        <attr name="shadowMarginLeft" format="dimension"/>
        <attr name="shadowMarginRight" format="dimension"/>
        <attr name="shadowMarginBottom" format="dimension"/>
        <attr name="cornerRadius" format="dimension"/>
        <attr name="cornerRadiusTL" format="dimension"/>
        <attr name="cornerRadiusTR" format="dimension"/>
        <attr name="cornerRadiusBL" format="dimension"/>
        <attr name="cornerRadiusBR" format="dimension"/>
        <attr name="foregroundColor" format="color"/>
        <attr name="shadowColor" format="color"/>
        <attr name="shadowDx" format="float"/>
        <attr name="shadowDy" format="float"/>
        <attr name="shadowRadius" format="dimension"/>
        <attr name="backgroundColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="ShadowView_Layout">
        <attr name="layout_gravity">
            <flag name="top"
                value="0x30"/>
            <flag name="bottom"
                value="0x50"/>
            <flag name="left"
                value="0x03"/>
            <flag name="right"
                value="0x05"/>
            <flag name="center_vertical"
                value="0x10"/>
            <flag name="fill_vertical"
                value="0x70"/>
            <flag name="center_horizontal"
                value="0x01"/>
            <flag name="fill_horizontal"
                value="0x07"/>
            <flag name="center"
                value="0x11"/>
            <flag name="fill"
                value="0x77"/>
            <flag name="clip_vertical"
                value="0x80"/>
            <flag name="clip_horizontal"
                value="0x08"/>
            <flag name="start"
                value="0x00800003"/>
            <flag name="end"
                value="0x00800005"/>
        </attr>
    </declare-styleable>





    <declare-styleable name="ExpandableTextView">
        <attr name="hnc_text" format="string" />
        <attr name="hnc_maxLine" format="integer" />
        <attr name="hnc_textColor" format="color" />
        <attr name="hnc_moreLessTextColor" format="color" />
        <attr name="hnc_textSize" format="dimension" />
        <attr name="hnc_moreLessTextSize" format="dimension" />
        <attr name="hnc_moreLessShow" format="boolean" />
        <attr name="hnc_moreLessGravity">
            <flag name="left" value="3" />
            <flag name="right" value="5" />
        </attr>
        <attr name="hnc_animationDuration" format="integer" />
        <attr name="hnc_moreLessAllCaps" format="boolean" />
        <attr name="hnc_moreLessTextStyle">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
        <attr name="hnc_TextStyle">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
        <attr name="hnc_defaultExpand" format="boolean" />
    </declare-styleable>



    <declare-styleable name="RatingReviews">

        <attr name="style" format="enum">
            <enum name="style1" value="1" />
            <enum name="style2" value="2" />
        </attr>

        <attr name="color" format="color" />
        <attr name="rounded" format="boolean"/>
        <attr name="gradiant" format="integer" />
        <attr name="width" format="dimension" />
        <attr name="text_size" format="dimension" />
        <attr name="text_color" format="color" />
        <attr name="spaces" format="dimension" />
        <attr name="max_value" format="integer" />
        <attr name="show_label" format="boolean" />
        <attr name="show_raters" format="boolean" />
        <attr name="animation" format="boolean" />

    </declare-styleable>


    <declare-styleable name="RoundedImageView">
        <attr name="riv_corner_radius" format="dimension" />
        <attr name="riv_corner_radius_top_left" format="dimension" />
        <attr name="riv_corner_radius_top_right" format="dimension" />
        <attr name="riv_corner_radius_bottom_left" format="dimension" />
        <attr name="riv_corner_radius_bottom_right" format="dimension" />
        <attr name="riv_border_width" format="dimension" />
        <attr name="riv_border_color" format="color" />
        <attr name="riv_mutate_background" format="boolean" />
        <attr name="riv_oval" format="boolean" />
        <attr name="android:scaleType" />
        <attr name="riv_tile_mode">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
        <attr name="riv_tile_mode_x">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
        <attr name="riv_tile_mode_y">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
    </declare-styleable>

</resources>